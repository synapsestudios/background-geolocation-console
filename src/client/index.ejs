<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <title>Background Geolocation Console</title>
    <link
      href="//fonts.googleapis.com/icon?family=Material+Icons"
      rel="stylesheet"
    />
    <link href="//fonts.googleapis.com/css?family=Roboto" rel="stylesheet" />
    <style>
      body {
        margin: 0;
      }
      .org-token-dialog .list-row-item {
        width: 100% !important;
      }
      .org-token-dialog .ReactVirtualized__Grid__innerScrollContainer {
        max-width: none !important;
      }
      .pane {
        height: 100%;
        position: fixed;
        z-index: 1;
        top: 50;
        overflow-x: hidden;
        padding-top: 20px;
      }
      .left {
        left: 0;
        width: 30%;
        border-right: 1px solid black;
        cursor: pointer;
      }
      .right {
        right: 0;
        width: 70%;
        overflow-y: scroll;
      }
    </style>

    <!-- Google Analytics -->
    <% if (htmlWebpackPlugin.options.GOOGLE_ANALYTICS_ID) { %>
    <script>
      (function(i, s, o, g, r, a, m) {
        i["GoogleAnalyticsObject"] = r;
        (i[r] =
          i[r] ||
          function() {
            (i[r].q = i[r].q || []).push(arguments);
          }),
          (i[r].l = 1 * new Date());
        (a = s.createElement(o)), (m = s.getElementsByTagName(o)[0]);
        a.async = 1;
        a.src = g;
        m.parentNode.insertBefore(a, m);
      })(
        window,
        document,
        "script",
        "https://www.google-analytics.com/analytics.js",
        "ga"
      );
      ga(
        "create",
        "<%= htmlWebpackPlugin.options.GOOGLE_ANALYTICS_ID %>",
        "auto"
      );
      ga("send", "pageview");
    </script>
    <% } %>

    <!-- Pure Chat widget -->
    <% if (htmlWebpackPlugin.options.PURE_CHAT_ID) { %>
    <script type="text/javascript" data-cfasync="false">
      window.purechatApi = {
        l: [],
        t: [],
        on: function() {
          this.l.push(arguments);
        }
      };
      (function() {
        var done = false;
        var script = document.createElement("script");
        script.async = true;
        script.type = "text/javascript";
        script.src = "https://app.purechat.com/VisitorWidget/WidgetScript";
        document
          .getElementsByTagName("HEAD")
          .item(0)
          .appendChild(script);
        script.onreadystatechange = script.onload = function(e) {
          if (
            !done &&
            (!this.readyState ||
              this.readyState == "loaded" ||
              this.readyState == "complete")
          ) {
            var w = new PCWidget({
              c: "<%= htmlWebpackPlugin.options.PURE_CHAT_ID %>",
              f: true
            });
            done = true;
          }
        };
      })();
    </script>
    <% } %>

    <!-- GOOGLE_MAPS_API_KEY -->
    <% if (htmlWebpackPlugin.options.GOOGLE_MAPS_API_KEY) { %>
    <script type="text/javascript">
      window.GOOGLE_MAPS_API_KEY =
        "<%= htmlWebpackPlugin.options.GOOGLE_MAPS_API_KEY %>";
    </script>
    <% } %>
  </head>

  <body>
    <div id="app-container"></div>
  </body>
</html>
